   "source": [
    "def factorial(n):\n",
    "    if n != 0:\n",
    "        return n * factorial(n-1)\n",
    "    else:\n",
    "        return 1\n",
    "\n",
    "fact = factorial(num)\n",
    "print(f\"fcatorial of {num} is {fact}\")\n"
   ]
